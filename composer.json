{
  "name": "easteregg/commentable",
  "description": "Commentable is a comments model for Laravel 5.",
  "type": "Package",
  "license": "proprietary",
  "authors": [
    {
      "name": "Mohammad Khavari",
      "email": "khavari.mvc@gmail.com"
    }
  ],
  "require": {
    "easteregg/setting": "~5.2"
  },
  "autoload": {
    "psr-4": {
      "Easteregg\\Comment\\": "src"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^3.2",
    "phpunit/phpunit": "^5.7",
    "laravel/framework": "~5.2",
    "doctrine/dbal": "^2.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Easteregg\\Comment\\CommentServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "git",
      "url":  "git@gitlab.com:easteregg/setting.git"
    }
  ]
}
