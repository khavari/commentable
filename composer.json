{
  "name": "easteregg/commentable",
  "description": "Commentable is a comments model for Laravel 5.",
  "type": "Package",
  "license": "proprietary",
  "authors": [
    {
      "name": "Mohammad Khavari",
      "email": "khavari.mvc@gmail.com"
    }
  ],
  "require": {
    "laravel/framework": "5.5.*",
    "doctrine/dbal": "~2.5",
    "easteregg/setting": "dev-master"

  },
  "autoload": {
    "psr-4": {
      "Easteregg\\Comment\\": "src"
    }
  },
  "require-dev": {
    "fzaninotto/faker": "1.5.*@dev",
    "orchestra/testbench": "3.5.*",
    "phpunit/phpunit": "6.*",
    "phpstan/phpstan": "^0.9.0@dev"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Easteregg\\Comment\\CommentServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "git",
      "url":  "git@gitlab.com:easteregg/setting.git"
    }
  ]
}
